

<div class="row">
<div class="col-md-6">
<h1>All Songs</h1>
</div>
<div class="col-md-6"></div>
</div>

<div class="row">

<table>
   <div class="panel panel-default">
      <table class="table">
  <thead>
    <tr>
      <th>Image</th>
      <th>Langauge</th>
      <th>Music category</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @music_listings.each do |music_listing| %>
    <td><%=link_to image_tag(music_listing.avatar.url(:thumb)), music_listing%></td>
        <td><%= music_listing.language.language %></td>
          <td><%= music_listing.music_category.category %></td>
        <td><%= link_to 'Show', music_listing %></td>
       
      </tr>
    <% end %>
  </tbody>
</div>
</table>

<br>
</div>

<%= link_to 'Create a new song listing', new_music_listing_path %>